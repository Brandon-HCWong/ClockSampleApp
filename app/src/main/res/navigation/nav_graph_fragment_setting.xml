<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_fragment_setting"
    app:startDestination="@id/settingFragment">

    <fragment
        android:id="@+id/settingFragment"
        android:name="com.brandonhc.clocksampleapp.ui.fragment.SettingFragment"
        android:label="SettingFragment"
        tools:layout="@layout/fragment_setting">

        <action
            android:id="@+id/action_settingFragment_to_editFragment"
            app:destination="@id/editFragment"/>
    </fragment>

    <fragment
        android:id="@+id/editFragment"
        android:name="com.brandonhc.clocksampleapp.ui.fragment.EditFragment"
        android:label="EditFragment"
        tools:layout="@layout/fragment_edit">
    </fragment>

</navigation>